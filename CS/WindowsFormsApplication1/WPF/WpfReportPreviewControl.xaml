<UserControl x:Class="WindowsFormsApplication1.WPF.WpfReportPreviewControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:dxp="http://schemas.devexpress.com/winfx/2008/xaml/printing" 
             xmlns:local="clr-namespace:WindowsFormsApplication1.WPF">
    <dxp:DocumentPreview Name="preview">
        <dxp:DocumentPreview.Model>
            <local:ReportPreviewModel />
        </dxp:DocumentPreview.Model>
    </dxp:DocumentPreview>
</UserControl>
